{
  "name": "nest-boilerplate",
  "version": "1.0.0",
  "description": "VivifyIdeas Nest Boilerplate",
  "author": "Nikola Spalevic",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "cross-env NODE_ENV=development nodemon",
    "start:debug": "cross-env NODE_ENV=development nodemon --config nodemon-debug.json",
    "start:prod": "node dist/main.js",
    "clear_jest": "jest --clearCache",
    "test": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --verbose --detectOpenHandles --forceExit",
    "test:cov": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --coverage --detectOpenHandles --forceExit",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "webpack": "webpack --config webpack.config.js",
    "ts-typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "migration:create": "typeorm migration:create -d src/migrations",
    "migration:run": "yarn ts-typeorm migration:run",
    "migration:revert": "yarn ts-typeorm migration:revert"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.6.1",
    "@nestjs/bull": "^0.5.3",
    "@nestjs/common": "^8.0.6",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/event-emitter": "^1.1.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.6",
    "@nestjs/platform-socket.io": "^8.4.4",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/typeorm": "^8.0.2",
    "@nestjs/websockets": "^8.4.4",
    "@types/express": "^4.17.13",
    "@types/pdfkit": "^0.12.4",
    "bcrypt": "^5.0.1",
    "botclient": "^0.0.6",
    "caver-js": "^1.8.0",
    "ccxt": "^1.76.31",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "coingecko-api-v3": "^0.0.13",
    "country-state-city": "^3.0.1",
    "cross-env": "^7.0.3",
    "delay": "^5.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.3",
    "jest-websocket-mock": "^2.3.0",
    "js-big-decimal": "^1.3.4",
    "level": "^8.0.0",
    "libphonenumber-js": "^1.9.48",
    "moment": "^2.29.1",
    "nestjs-typeorm-paginate": "^3.1.3",
    "otp-generator": "^4.0.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pdfkit-table": "^0.1.86",
    "pg": "^8.7.3",
    "redis-server": "^1.2.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "short-unique-id": "^4.4.4",
    "socket.io": "^4.4.1",
    "speakeasy": "^2.0.0",
    "typeorm": "^0.2.30",
    "typeorm-extension": "^1.1.0",
    "typescript": "^4.4.2",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.0.6",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.15.8",
    "@types/cron": "^1.7.3",
    "@types/jest": "^27.0.1",
    "@types/node": "^14.18.15",
    "@types/otp-generator": "^4.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "jest": "^27.1.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "supertest": "6.1.6",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
